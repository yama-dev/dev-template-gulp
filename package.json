{
  "name": "dev-template-gulp",
  "version": "1.14.2",
  "description": "Development environment for web site development",
  "keywords": [
    "web",
    "gulp",
    "frontend",
    "template"
  ],
  "author": "yama-dev",
  "license": "MIT",
  "main": "gulpfile.esm.js",
  "scripts": {
    "start": "npm install -s && npm run dev",
    "dev": "npm run develop",
    "dev:basic": "npm run develop -- --source _src_basic --sourceBuild dist_basic",
    "dev:ejs": "npm run develop -- --source _src_ejs/ --sourceBuild dist_ejs",
    "dev:webpack": "npm run develop -- --source _src_webpack/ --sourceBuild dist_webpack --webpack true --webpackConfig ./_src_webpack/webpack.config.js",
    "dev:php": "npm run develop:php -- --source _src_php/ --sourceBuild dist_php",
    "dev:twig": "npm run develop:twig -- --source _src_twig/ --sourceBuild dist_twig",
    "php": "npm run develop:php",
    "twig": "npm run develop:twig",
    "develop": "gulp --develop",
    "develop:php": "gulp --php --develop",
    "develop:twig": "gulp --twig --develop",
    "prod": "gulp prod --production",
    "preview": "gulp prod --production",
    "build:basic": "npm run build -- --source _src_basic/ --sourceBuild dist_basic",
    "build:ejs": "npm run build -- --source _src_ejs/ --sourceBuild dist_ejs",
    "build:webpack": "npm run build -- --source _src_webpack/ --sourceBuild dist_webpack --webpack true --webpackConfig ./_src_webpack/webpack.config.js",
    "build:webpack:config": "npm run build -- --config ./_src_webpack/.config.json",
    "build": "gulp build --production",
    "deploy": "gulp deploy --production",
    "check": "npm outdated",
    "test": "npm install -s && npm run build:basic && npm run build:ejs && npm run build:webpack && npm run build:webpack:config && npm run check"
  },
  "engines": {
    "node": ">=14.0.0 <=18.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yama-dev/dev-template-gulp"
  },
  "bugs": {
    "url": "https://github.com/yama-dev/dev-template-gulp/issues",
    "email": "tatsuya.yamamoto69@gmail.com"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.21.0",
    "@yama-dev/data-collector": "^0.6.0",
    "@yama-dev/data-template": "^0.6.0",
    "@yama-dev/dev-template-gulp-core": "yama-dev/dev-template-gulp-core.git#v0.0.0",
    "@yama-dev/gulp-stream-util": "^0.3.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.27.4",
    "css-declaration-sorter": "^6.3.1",
    "cssnano": "^5.1.15",
    "del": "^6.0.0",
    "dotenv": "^16.0.1",
    "eslint": "^7.31.0",
    "esm": "https://github.com/jsg2021/esm/releases/download/v3.x.x-pr883/esm-3.x.x-pr883.tgz",
    "glob": "^7.1.7",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cached": "^1.1.1",
    "gulp-changed": "^4.0.3",
    "gulp-ejs": "^5.1.0",
    "gulp-eslint": "github:yama-dev/gulp-eslint#v7.20.0",
    "gulp-htmlhint": "^4.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-ignore": "^3.0.0",
    "gulp-javascript-obfuscator": "^1.1.6",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "~9.0.1",
    "gulp-prettier": "~4.0.0",
    "gulp-prompt": "^1.2.0",
    "gulp-pug": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "~5.1.0",
    "gulp-terser": "^2.0.1",
    "node-notifier": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-color-hex-alpha": "~9.0.2",
    "postcss-combine-media-query": "~1.0.1",
    "postcss-custom-media": "~9.1.2",
    "postcss-custom-selectors": "~7.1.2",
    "postcss-media-minmax": "~5.0.0",
    "sass": "~1.70.0",
    "webpack": "^5.90.0",
    "webpack-stream": "^7.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ]
    ],
    "retainLines": true,
    "comments": true
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not IE 11",
    "ios >= 12"
  ],
  "eslintConfig": {
    "env": {
      "amd": true,
      "browser": true,
      "es6": true,
      "jquery": true,
      "node": true
    },
    "rules": {
      "indent": [
        "error",
        2,
        {
          "outerIIFEBody": "off",
          "SwitchCase": 1
        }
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-console": "off"
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    }
  },
  "volta": {
    "node": "18.12.0"
  }
}
