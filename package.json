{
  "name": "dev-template-gulp",
  "version": "1.10.5",
  "description": "Development environment for web site development",
  "keywords": [
    "web",
    "gulp",
    "template"
  ],
  "author": "yama-dev",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "preinstall": "npm config set loglevel silent",
    "start": "npm install -s && npm run dev",
    "dev": "gulp --develop --silent",
    "develop": "gulp --develop --silent",
    "prod": "gulp prod --production --silent",
    "build": "gulp prod --production --silent",
    "release": "gulp prod --production --silent",
    "php": "gulp --php --develop --silent",
    "twig": "gulp --twig --develop --silent",
    "deploy": "gulp deploy --production --silent",
    "check": "npm outdated",
    "lint": "eslint gulpfile.js",
    "test": "npm run lint && gulp release",
    "test:twigmodule": "phpunit ./src/_twig/ModuleTest.php || true"
  },
  "engines": {
    "node": ">=6.0.0 <13.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yama-dev/dev-template-gulp"
  },
  "bugs": {
    "url": "https://github.com/yama-dev/dev-template-gulp/issues",
    "email": "tatsuya.yamamoto69@gmail.com"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/register": "^7.7.7",
    "@yama-dev/data-collector": "^0.6.0",
    "@yama-dev/data-template": "^0.5.0",
    "@yama-dev/gulp-stream-util": "^0.3.0",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "colors": "^1.4.0",
    "css-declaration-sorter": "^4.0.1",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "eslint": "^6.7.2",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cached": "^1.1.1",
    "gulp-ejs": "^5.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-htmlhint": "^3.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-ignore": "^3.0.0",
    "gulp-javascript-obfuscator": "^1.1.6",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-prompt": "^1.2.0",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-terser": "^1.4.0",
    "node-notifier": "^9.0.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "pixrem": "^5.0.0",
    "webpack": "^4.41.5",
    "webpack-stream": "^5.2.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ]
    ],
    "retainLines": true,
    "comments": true
  },
  "browserslist": [
    "ie >= 9",
    "ios >= 9",
    "android >= 4.4",
    "last 2 versions"
  ],
  "eslintConfig": {
    "env": {
      "amd": true,
      "browser": true,
      "es6": true,
      "jquery": true,
      "node": true
    },
    "rules": {
      "indent": [
        "error",
        2,
        {
          "outerIIFEBody": 0,
          "SwitchCase": 1
        }
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-console": "off"
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    }
  }
}
